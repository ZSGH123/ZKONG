package com.kba.entity;

public class KType {

	private String kTypeId;//	K币类型编号
	private String kTypeName;//	k币类型名称
	private double kExchangeRate;//	k币充值比率
	private double kWithdrawRate;//	k币提现比率	
	private String kRemark;  //k币类型备注
	public String getkTypeId() {
		return kTypeId;
	}
	public void setkTypeId(String kTypeId) {
		this.kTypeId = kTypeId;
	}
	public String getkTypeName() {
		return kTypeName;
	}
	@Override
	public String toString() {
		return "K币类型 [K币类型编号=" + kTypeId + ", k币类型名称=" + kTypeName + ", k币充值比率=" + kExchangeRate
				+ ", k币提现比率	=" + kWithdrawRate + ", k币类型备注=" + kRemark + ", toString()=" + super.toString()
				+ "]";
	}
	public void setkTypeName(String kTypeName) {
		this.kTypeName = kTypeName;
	}
	public double getkExchangeRate() {
		return kExchangeRate;
	}
	public void setkExchangeRate(double kExchangeRate) {
		this.kExchangeRate = kExchangeRate;
	}
	public double getkWithdrawRate() {
		return kWithdrawRate;
	}
	public void setkWithdrawRate(double kWithdrawRate) {
		this.kWithdrawRate = kWithdrawRate;
	}
	public String getkRemark() {
		return kRemark;
	}
	public void setkRemark(String kRemark) {
		this.kRemark = kRemark;
	}
}
