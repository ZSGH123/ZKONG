@CHARSET "UTF-8";

a {
	text-decoration: none;
	out-line: none;
}

a : hover {
	text-decoration: none;
	out-line: none;
}

a{ 
    text-decoration:none !important;
}

#left{
	position: fixed;
	left: 0px;
	top: 60px;
	height: 700px;
	width: 245px;
	font-size: 95%;
	border-radius:0px;
	background-color:  #2c2c2c;	
}

#left_button{
	position: fixed;
	top: 35%;
	left: 180px;
	width: 75px;
	height: 170px;
	border-radius: 37.5px;
	background-color:  #2c2c2c;	
}

#left_button2{
	float: right;
	width: 15px;
	height: 170px;
	text-align: center;
	line-height: 170px;
	color: #555;
}

#left_button2:hover{
	color: coral;
}

#left_model{
	position: fixed;
	left: 0px;
	top: 60px;
	height: 700px;
	width: 50px;
	color: white;
	background-color:  #2c2c2c;
	border-radius:0px;
	display: none;
}

#left_model_button{
	position: fixed;
	top: 35%;
	left: -15px;
	width: 75px;
	height: 170px;
	border-radius: 37.5px;
	background-color:  #2c2c2c;	
}

#left_model_button2{
	float: right;
	width: 15px;
	height: 170px;
	text-align: center;
	line-height: 170px;
	color: #555;
}

#left_model_button2:hover{
	color: coral;
}

#left_context{
	position: absolute;
	color: #d0d0d0;
	width: 240px;
	height: 700px;
	border-radius: 0px;
	/*婊氬姩鏉�*/
    overflow-y:scroll;
}
#left_context::-webkit-scrollbar {
	/*婊氬姩鏉℃暣浣撴牱寮�*/
	width: 8.5px;
	/*楂樺鍒嗗埆瀵瑰簲妯珫婊氬姩鏉＄殑灏哄*/
}
#left_context::-webkit-scrollbar-thumb {/*婊氬姩鏉￠噷闈㈠皬鏂瑰潡*/
        border-radius: 8px;
        background: #454545;
    }

.leftContext{
	width: 245px;
	height: 34px;
	line-height: 34px;
	border-bottom: solid;
	border-bottom-color: #272727;
	border-bottom-width: thin;
	color: #d0d0d0;
}

.left_context_head{
	width: 20px;
	height: 34px;
	margin-left: 30px;
	float: left;
	margin-right: 20px;
}

.leftContext:hover{
	color: #fff;
	background-color: #f60;
}

.left_context_right{
	float: right;
	width: 26px;
	height: 34px;
}

.left_context_middle{
	float: left;
	margin-top: 5px;
	text-align: center;
	width: 105px;
	height: 30px;
	line-height: 30px;
	border-bottom: solid;
	border-bottom-color: #f60;
	border-bottom-width: 1.5px;
}

#left_context_left{
	margin-left: 15px;
	background-color: #f60;
	border-radius: 0px;
}

#left_context_right{
	border-radius: 0px;
}

.biaoti{
	float: left;
	width: 232px;
	height: 35px;
	line-height: 35px;
	margin-left: 15px;
	color: #c6c6c6;
}

.biaoti:hover{
	color: #f60;
}

.biaoti div{
	float: left;
	width: 4px;
	height: 13px;
	margin-top: 11px;
	border-radius: 4px;
    background-color: #f60;
    margin-right: 10px;
}

.leftul1{
	float: left;
	width: 232px;
	margin-top: 0px;
	margin-left: 0px;
	padding-left: 15px;
}

.leftul1:hover{
	background-color: #222;
}

.leftul1 li{
	list-style: none;
	float: left;
	width: 66px;
	height: 23.5px;
	margin-right: 5px;
	margin-bottom: 5px;
	border-radius: 3px;
	background-color: #3f3f3f;
	text-align: center;
	line-height: 23.5px;
	font-size: 82%;
}

.leftul1 li:hover{
	color: #fff;
	background-color: #f60;
}

.leftul1 li a{
	height:23.5px;
	width:66px;
	color:#d0d0d0;
}

.leftul1 li a:hover{
	color: #fff;
}

#left_bottom{
	position: fixed;
	bottom: 0px;
	float: left;
	width: 240px;
	text-align: center;
	font-size: 82%;
	height: 40px;
	line-height: 40px;
	border-radius: 0px;
	background-color: #2c2c2c;
}

#left_bottom a{
	color: #d0d0d0;
	margin-left: 8px;
}

#left_bottom a:hover{
	color: #f60;
}

.liquanbu:hover{
	color:#fff;
}

