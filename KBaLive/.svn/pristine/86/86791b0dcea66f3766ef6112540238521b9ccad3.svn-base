package com.kba.entity;

import java.util.Date;

/**
 * 登录日志信息
 * @author 杨智能
 *时间：2019-1-13
 */
public class LoginLog {

	private String loginLogId;//登录日志编号
	
	private String userId;//用户编号
	
	private Date loginTime;//登录时间
	
	private String loginAddress;//登录地址
	
	private Integer loginState;//登录状态
	
	private String loginLogRemark;//登录日志备注
	public String getLoginLogId() {
		return loginLogId;
	}

	public void setLoginLogId(String loginLogId) {
		this.loginLogId = loginLogId;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	

	public Date getLoginTime() {
		return loginTime;
	}

	public void setLoginTime(Date loginTime) {
		this.loginTime = loginTime;
	}

	public String getLoginAddress() {
		return loginAddress;
	}

	public void setLoginAddress(String loginAddress) {
		this.loginAddress = loginAddress;
	}

	
	public Integer getLoginState() {
		return loginState;
	}

	public void setLoginState(Integer loginState) {
		this.loginState = loginState;
	}

	public String getLoginLogRemark() {
		return loginLogRemark;
	}

	public void setLoginLogRemark(String loginLogRemark) {
		this.loginLogRemark = loginLogRemark;
	}

	@Override
	public String toString() {
		return "登录日志信息 [登录日志编号=" + loginLogId + ", 用户编号=" + userId + ", 登录时间=" + loginTime
				+ ", 登录地址=" + loginAddress + ", 登录状态=" + loginState + ", 登录日志备注=" + loginLogRemark
				+ "]";
	}
	
}
