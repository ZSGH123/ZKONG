package com.kba.entity;

import java.util.Date;


/**
 * 上传视频信息
 * @author 单杰
 * 创建时间：2019-01-13
 * 修改时间：
 */
public class UploadVideoInfo {
	private String uploadVideoId;//上传视频编号
	private String uploadVideoName;//上传视频名称
	private String userId;//用户编号
	private String uploadVideoAddress;//上传的视频地址
	private String uploadVideoIcon;//上传的视频封面
	private Date uploadVideoTime;//上传视频的时间
	private String videoTypeId;//视频类型编号
	
	public String toString(){
		return "上传视频信息 [上传视频编号=" + uploadVideoId + ",上传视频名称=" + uploadVideoName + ",用户编号=" + userId + ",上传的视频地址=" + uploadVideoAddress 
				+ ",上传的视频封面=" + uploadVideoIcon + ",上传视频的时间=" + uploadVideoTime + ",视频类型编号=" + videoTypeId + "]";
	}
	
	public String getUploadVideoId() {
		return uploadVideoId;
	}
	public void setUploadVideoId(String uploadVideoId) {
		this.uploadVideoId = uploadVideoId;
	}
	public String getUploadVideoName() {
		return uploadVideoName;
	}
	public void setUploadVideoName(String uploadVideoName) {
		this.uploadVideoName = uploadVideoName;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getUploadVideoAddress() {
		return uploadVideoAddress;
	}
	public void setUploadVideoAddress(String uploadVideoAddress) {
		this.uploadVideoAddress = uploadVideoAddress;
	}
	public String getUploadVideoIcon() {
		return uploadVideoIcon;
	}
	public void setUploadVideoIcon(String uploadVideoIcon) {
		this.uploadVideoIcon = uploadVideoIcon;
	}
	public Date getUploadVideoTime() {
		return uploadVideoTime;
	}
	public void setUploadVideoTime(Date uploadVideoTime) {
		this.uploadVideoTime = uploadVideoTime;
	}
	public String getVideoTypeId() {
		return videoTypeId;
	}
	public void setVideoTypeId(String videoTypeId) {
		this.videoTypeId = videoTypeId;
	}
}
