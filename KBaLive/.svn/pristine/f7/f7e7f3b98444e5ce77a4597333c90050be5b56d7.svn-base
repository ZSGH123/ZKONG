package com.kba.vo;

/**
 * 直播间所有信息类
 * @author Administrator
 *
 */
public class LiveHomeMessages {
    
    private String liveRoomId; //直播间编号
	
	private String userName; //主播名字
	
	private String livePushCode;//直播推流码（file文件名）
	
	private String liveRoomAnnouncement;//直播间公告
	
	private String liveRoomTilie;//直播间标题
	
	private String userIcon; //主播头像
	
	private String liveContextName; //直播内容
	
	private String liveTypeName; //直播版块
	
	private String anchorLevelId;  //主播等级编号
	
	private String anchorBookedNumber;  //主播关注数
	
	private String anchorInfoRemark;  //主播信息备注(即官方认证)

	public LiveHomeMessages() {
		
	}

	public LiveHomeMessages(String liveRoomId, String userName, String livePushCode, String liveRoomAnnouncement,
			String liveRoomTilie, String userIcon, String liveTypeName, String liveContentImgURL, String anchorLevelId,
			String anchorBookedNumber) {
		super();
		this.liveRoomId = liveRoomId;
		this.userName = userName;
		this.livePushCode = livePushCode;
		this.liveRoomAnnouncement = liveRoomAnnouncement;
		this.liveRoomTilie = liveRoomTilie;
		this.userIcon = userIcon;
		this.liveTypeName = liveTypeName;
		this.anchorLevelId = anchorLevelId;
		this.anchorBookedNumber = anchorBookedNumber;
	}

	public String getLiveRoomId() {
		return liveRoomId;
	}

	public void setLiveRoomId(String liveRoomId) {
		this.liveRoomId = liveRoomId;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getLivePushCode() {
		return livePushCode;
	}

	public void setLivePushCode(String livePushCode) {
		this.livePushCode = livePushCode;
	}

	public String getLiveRoomAnnouncement() {
		return liveRoomAnnouncement;
	}

	public void setLiveRoomAnnouncement(String liveRoomAnnouncement) {
		this.liveRoomAnnouncement = liveRoomAnnouncement;
	}

	public String getLiveRoomTilie() {
		return liveRoomTilie;
	}

	public void setLiveRoomTilie(String liveRoomTilie) {
		this.liveRoomTilie = liveRoomTilie;
	}

	public String getUserIcon() {
		return userIcon;
	}

	public void setUserIcon(String userIcon) {
		this.userIcon = userIcon;
	}

	public String getLiveTypeName() {
		return liveTypeName;
	}

	public void setLiveTypeName(String liveTypeName) {
		this.liveTypeName = liveTypeName;
	}

	public String getAnchorLevelId() {
		return anchorLevelId;
	}

	public void setAnchorLevelId(String anchorLevelId) {
		this.anchorLevelId = anchorLevelId;
	}

	public String getAnchorBookedNumber() {
		return anchorBookedNumber;
	}

	public void setAnchorBookedNumber(String anchorBookedNumber) {
		this.anchorBookedNumber = anchorBookedNumber;
	}

	public String getLiveContextName() {
		return liveContextName;
	}

	public void setLiveContextName(String liveContextName) {
		this.liveContextName = liveContextName;
	}

	public String getAnchorInfoRemark() {
		return anchorInfoRemark;
	}

	public void setAnchorInfoRemark(String anchorInfoRemark) {
		this.anchorInfoRemark = anchorInfoRemark;
	}

	@Override
	public String toString() {
		return "LiveHomeMessages [liveRoomId=" + liveRoomId + ", userName=" + userName + ", livePushCode="
				+ livePushCode + ", liveRoomAnnouncement=" + liveRoomAnnouncement + ", liveRoomTilie=" + liveRoomTilie
				+ ", userIcon=" + userIcon + ", liveContextName=" + liveContextName + ", liveTypeName=" + liveTypeName
				+ ", anchorLevelId=" + anchorLevelId + ", anchorBookedNumber=" + anchorBookedNumber
				+ ", anchorInfoRemark=" + anchorInfoRemark + "]";
	}
	
}
