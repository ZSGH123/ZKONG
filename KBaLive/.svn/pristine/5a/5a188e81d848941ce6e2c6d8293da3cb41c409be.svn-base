package com.kba.entity;

import java.util.Date;


/**
 * 用户订阅
 * @author 单杰
 * 创建时间：2019-01-13
 * 修改时间：
 */
public class UserBook {
	private String userBookId;//用户订阅编号
	private String anchorId;//主播编号
	private String userId;//用户编号
	private Date userBookTime;//用户订阅时间
	private Date userTakeOffTime;//用户取关时间
	private String userBookRemark;//用户订阅备注
	
	public String toString(){
		return "用户订阅 [用户订阅编号=" + userBookId + ",主播编号=" + anchorId + ",用户编号=" + userId + ",用户订阅时间=" + userBookTime 
				+ ",用户取关时间=" + userTakeOffTime + ",用户订阅备注=" + userBookRemark + "]";
	}
	
	public String getUserBookId() {
		return userBookId;
	}
	public void setUserBookId(String userBookId) {
		this.userBookId = userBookId;
	}
	public String getAnchorId() {
		return anchorId;
	}
	public void setAnchorId(String anchorId) {
		this.anchorId = anchorId;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public Date getUserBookTime() {
		return userBookTime;
	}
	public void setUserBookTime(Date userBookTime) {
		this.userBookTime = userBookTime;
	}
	public Date getUserTakeOffTime() {
		return userTakeOffTime;
	}
	public void setUserTakeOffTime(Date userTakeOffTime) {
		this.userTakeOffTime = userTakeOffTime;
	}
	public String getUserBookRemark() {
		return userBookRemark;
	}
	public void setUserBookRemark(String userBookRemark) {
		this.userBookRemark = userBookRemark;
	}
}
