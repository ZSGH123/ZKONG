.user-myexchange-title {
    width: 100%;
    margin-bottom: 5px;
    background: #f5f5f5;
}
.user-myexchange-icon {
   	float: left;
    display: inline-block;
    width: 32px;
    height: 32px;
    margin: 0 5px 0 20px;
    vertical-align: middle;
    background-position: 0 0;
    background-image: url(../img/兑换.png);
}
.user-myexchange-title h4{
	float: left;
}
.earn_tab_tit {
    height: 62px;
    border-bottom: 2px solid #ebebeb;
    padding-top: 6px;
}
.earn_tab_tit dl {
    overflow: hidden;
    margin-bottom: 0px;
	padding-top: 15px;
}
.earn_tab_tit dl dd.fr {
    float: right;
}
.earn_tab_tit dl dd {
    cursor: auto;
}
.earn_tab_tit dl dd {
    float: left;
    width: 67px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    cursor: pointer;
    color: #c9c9c9;
    position: relative;
}
.earn_tab_tit dl dd a {
    display: block;
    color: #333;
    text-decoration: none;
}
.earn_tab_tit dl dd.active {
    background: #fff;
    border-top: 2px solid #ff8a00;
    border-left: 1px solid #ebebeb;
    border-right: 1px solid #ebebeb;
    color: #ff8a00;
    bottom: -2px;
}
.earn_tab_tit dl dd.active a {
    color: #ff8a00;
}
#tool-info-jin{
	float: left;
	color:  #f60;

}
#tool-info-yin{
	float: right;
	color:  #f60;

}
#exchange-input{
	float: left;
}
.tool-btn{
	margin-top: 5px;
	margin-left: 365px;
	color: #fff;
	background-color:  #EC971F;
	border-color: #EC971F;
}
#exchange_type{
	margin-top: 10px;
}
.exchange_nr {
    padding-top: 18px;
}
.exchange_nr table {
    width: 100%;
    border-collapse: collapse;
}
.exchange_nr table tr {
    height: 40px;
}
.exchange_nr table th {
    padding-left: 18px;
    background: #f5f5f5;
    border: 1px solid #e4e4e4;
    color: #555;
    font-size: 14px;
    font-weight: normal;
    position: relative;
}
.exchange_nr table td {
    padding-left: 18px;
    border: 1px solid #e4e4e4;
    position: relative;
}