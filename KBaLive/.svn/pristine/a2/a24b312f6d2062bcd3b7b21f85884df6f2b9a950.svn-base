package com.kba.entity;
/**
 * 直播类容
 * @author 钟强
 * 创建时间：2019-01-13
 * 修改时间：
 */
public class LiveContent {
//	直播内容编号	live_content_id	varchar2(5)
//	直播内容名称	live_content_name	varchar2(30)
//	直播类型编号	live_type_id	varchar2(5)
//	直播内容备注	live_content_remark	varchar2(200)
    private String liveContentId;//	直播内容编号
    private String liveContentName;//	直播内容名称
	private String liveTypeId;//	直播类型编号
	private String liveContentImgURL; //直播内容的插图
	private String liveContentRemark;//	直播内容备注
	@Override
	public String toString() {
		return "直播类容 [直播内容编号=" + liveContentId + ", 直播内容名称=" + liveContentName + ", 直播类型编号="
				+ liveTypeId + ", 直播内容的插图=" + liveContentImgURL + ", 直播内容备注=" + liveContentRemark
				+ ", toString()=" + super.toString() + "]";
	}
	public String getLiveContentImgURL() {
		return liveContentImgURL;
	}
	public void setLiveContentImgURL(String liveContentImgURL) {
		this.liveContentImgURL = liveContentImgURL;
	}
	public String getLiveContentId() {
		return liveContentId;
	}
	public void setLiveContentId(String liveContentId) {
		this.liveContentId = liveContentId;
	}
	public String getLiveContentName() {
		return liveContentName;
	}
	public void setLiveContentName(String liveContentName) {
		this.liveContentName = liveContentName;
	}
	public String getLiveTypeId() {
		return liveTypeId;
	}
	public void setLiveTypeId(String liveTypeId) {
		this.liveTypeId = liveTypeId;
	}
	public String getLiveContentRemark() {
		return liveContentRemark;
	}
	public void setLiveContentRemark(String liveContentRemark) {
		this.liveContentRemark = liveContentRemark;
	}
	
	
}
