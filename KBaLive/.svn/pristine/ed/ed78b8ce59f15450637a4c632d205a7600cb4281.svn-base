package com.kba.entity;

public class UserWeiboInfo {

	public String userWeiboId; //微博用户编号
	public String userWeiboName;//微博用户名
	public String userWeiboIcon;//微博用户头像地址
	public String userId;//用户编号	
	public String  userWeiboRemark;//微博用户备注	
	@Override
	public String toString() {
		return "微博用户表[微博用户编号=" + userWeiboId + ", 微博用户名="
				+ userWeiboName + ", 微博用户头像地址=" + userWeiboIcon
				+ ", 用户编号=" + userId + ", 微博用户备注=" + userWeiboRemark
				+ "]";
	}
	public String getUserWeiboId() {
		return userWeiboId;
	}
	public void setUserWeiboId(String userWeiboId) {
		this.userWeiboId = userWeiboId;
	}
	public String getUserWeiboName() {
		return userWeiboName;
	}
	public void setUserWeiboName(String userWeiboName) {
		this.userWeiboName = userWeiboName;
	}
	public String getUserWeiboIcon() {
		return userWeiboIcon;
	}
	public void setUserWeiboIcon(String userWeiboIcon) {
		this.userWeiboIcon = userWeiboIcon;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getUserWeiboRemark() {
		return userWeiboRemark;
	}
	public void setUserWeiboRemark(String userWeiboRemark) {
		this.userWeiboRemark = userWeiboRemark;
	}
}
